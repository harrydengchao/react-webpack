{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8", "iOS >= 8", "Android >= 4.1"]
      }
    }],
    "react",
    "stage-2"
  ],
  "plugins": [
    ["import", {
      "libraryName": "antd-mobile",
      "style": "css"
    }],
    "transform-class-properties",
    "transform-runtime"
  ]
}
