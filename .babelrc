{
  "presets": [
    "react",
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 0.2%", "last 5 versions", "ie >= 7"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime"],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
